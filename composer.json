{
    "name": "networking/init-cms-bundle",
    "description": "This Bundle is a basic cms based on symfony",
    "keywords": ["symfony", "cms"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yorkie Chadwick",
            "homepage": "http://web.networking.ch",
            "email": "y.chadwick@networking.ch"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "friendsofsymfony/ckeditor-bundle": "^1.1",
        "jms/serializer-bundle": "^3.4",
        "knplabs/knp-paginator-bundle": "^4.0",
        "lexik/translation-bundle": "*",
        "oneup/flysystem-bundle": "^3.0",
        "oneup/uploader-bundle": "^2.1",
        "phpfastcache/phpfastcache": "^3.0",
        "mopa/bootstrap-bundle": "^3.3",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^6.0",
        "sonata-project/admin-bundle": "^3.54",
        "sonata-project/doctrine-orm-admin-bundle": "^3.10",
        "sonata-project/easy-extends-bundle": "^2.5",
        "sonata-project/formatter-bundle": "^4.1",
        "sonata-project/intl-bundle": "^2.7",
        "sonata-project/media-bundle": "^3.21",
        "sonata-project/notification-bundle": "^3.6",
        "sonata-project/user-bundle": "^4.5",
        "stof/doctrine-extensions-bundle": "~1.1",
        "gedmo/doctrine-extensions": "v2.4",
        "symfony/acl-bundle": "^1.0",
        "symfony/asset": "^3.4 || ^4.0",
        "symfony/console": "^3.4 || ^4.0",
        "symfony/expression-language": "^3.4 || ^4.0",
        "symfony/form": "^3.4 || ^4.0",
        "symfony/framework-bundle": "^3.4 || ^4.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/proxy-manager-bridge": "^3.4 || ^4.0",
        "symfony/security-bundle": "^3.4 || ^4.0",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^3.4 || ^4.0",
        "symfony/twig-bundle": "^3.4 || ^4.0",
        "symfony/validator": "^3.4 || ^4.0",
        "symfony/webpack-encore-bundle": "^1.5",
        "symfony/yaml": "^3.4 || ^4.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony-cmf/routing-bundle": "^2.0",
        "twbs/bootstrap": ">2.0,<4.0-dev",
        "twig/twig": "^1.0||^2.0"
    },
    "scripts": {
    },
    "autoload": {
        "psr-4": {
            "Networking\\InitCmsBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Networking\\InitCmsBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "require-dev": {
        "dg/bypass-finals": "^1.1"
    }
}
