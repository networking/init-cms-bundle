{
  "name": "networking/init-cms-bundle",
  "description": "This Bundle is a basic cms based on symfony",
  "keywords": [
    "symfony",
    "cms"
  ],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Yorkie Chadwick",
      "homepage": "http://web.networking.ch",
      "email": "y.chadwick@networking.ch"
    }
  ],
  "require": {
    "php": ">=7.4 || ^8.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "friendsofsymfony/ckeditor-bundle": "^2.2",
    "jms/serializer-bundle": "^3.4 || ^4.0",
    "knplabs/knp-paginator-bundle": "^5.9.0",
    "lexik/translation-bundle": "6.*",
    "oneup/flysystem-bundle": "^4.0",
    "oneup/uploader-bundle": "^4.0",
    "mopa/bootstrap-bundle": "dev-master",
    "sensio/framework-extra-bundle": "^6.1",
    "sonata-project/admin-bundle": "^4.17.0",
    "sonata-project/doctrine-orm-admin-bundle": "^4.7",
    "sonata-project/media-bundle": "^4.5",
    "sonata-project/user-bundle": "^5.3",
    "sonata-project/google-authenticator": "^2.0",
    "gedmo/doctrine-extensions": "^v3.8",
    "symfony/http-foundation": "^4.4|^5.3|^6.0",
    "symfony/http-kernel": "^4.4|^5.3|^6.0",
    "symfony/routing": "^4.4|^5.3|^6.0",
    "symfony/acl-bundle": "^2.3",
    "symfony/asset": "^4.4 || ^5.4 || ^6.0",
    "symfony/console": "^4.4 || ^5.4 || ^6.0",
    "symfony/expression-language": "^4.4 || ^5.4 || ^6.0",
    "symfony/form": "^4.4 || ^5.4 || ^6.0",
    "symfony/framework-bundle": "^4.4.1|^5.0|^6.0",
    "symfony/monolog-bundle": "^3.8.0",
    "symfony/orm-pack": "^2.0",
    "symfony/proxy-manager-bridge": "^4.4 || ^5.4 || ^6.0",
    "symfony/security-bundle": "^4.4 || ^5.4 || ^6.0",
    "symfony/serializer-pack": "^1.0",
    "symfony/translation": "^4.4 || ^5.4 || ^6.0",
    "symfony/twig-bundle": "^4.4 || ^5.4 || ^6.0",
    "symfony/validator": "^4.4 || ^5.4 || ^6.0",
    "symfony/webpack-encore-bundle": "^1.5",
    "symfony/yaml": "^4.4 || ^5.4 || ^6.0",
    "symfony/polyfill-apcu": "^1.0",
    "symfony-cmf/routing-bundle": "^2.0 || ^3.0",
    "twbs/bootstrap-sass": "^3.4",
    "twig/twig": "^2.12.1 || ^3.0"
  },
  "scripts": {
  },
  "autoload": {
    "psr-4": {
      "Networking\\InitCmsBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Networking\\InitCmsBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "5.x-dev"
    }
  },
  "require-dev": {
    "dg/bypass-finals": "^1.1",
    "symfony/phpunit-bridge": "^5.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:networking/MopaBootstrapBundle.git"
    }
  ]
}
