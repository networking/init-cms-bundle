{
    "name": "networking/init-cms-bundle",
    "description": "This Bundle is a basic cms based on symfony",
    "keywords": ["symfony", "cms"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yorkie Chadwick",
            "homepage": "http://web.networking.ch",
            "email": "y.chadwick@networking.ch"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "jms/serializer-bundle": "^3.4",
        "knplabs/knp-paginator-bundle": "^4.0",
        "lexik/translation-bundle": "^4.0",
        "oneup/flysystem-bundle": "^3.0",
        "oneup/uploader-bundle": "^2.1.6",
        "mopa/bootstrap-bundle": "^3.4",
        "sensio/framework-extra-bundle": "^5.1",
        "sonata-project/admin-bundle": "^3.71.1",
        "sonata-project/doctrine-orm-admin-bundle": "^3.19",
        "sonata-project/intl-bundle": "^2.7",
        "sonata-project/media-bundle": "^3.25",
        "sonata-project/user-bundle": "^4.6",
        "sonata-project/block-bundle": "^3.20",
        "sonata-project/datagrid-bundle": "^2.4",
        "sonata-project/doctrine-extensions": "^1.0.2",
        "sonata-project/exporter": "^2.3",
        "sonata-project/form-extensions": "^1.5",
        "sonata-project/google-authenticator": "^2.0",
        "gedmo/doctrine-extensions": "^2.4",
        "symfony/acl-bundle": "^1.0",
        "symfony/asset": "^3.4 || ^4.4",
        "symfony/console": "^3.4 || ^4.4",
        "symfony/expression-language": "^3.4 || ^4.4",
        "symfony/form": "^3.4 || ^4.4",
        "symfony/framework-bundle": "^3.4.26 || ^4.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/proxy-manager-bridge": "^3.4 || ^4.4",
        "symfony/security-bundle": "^3.4 || ^4.4",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^3.4 || ^4.4",
        "symfony/twig-bundle": "^3.4 || ^4.4",
        "symfony/validator": "^3.4 || ^4.4",
        "symfony/webpack-encore-bundle": "^1.5",
        "symfony/yaml": "^3.4 || ^4.4",
        "symfony/polyfill-apcu": "^1.0",
        "symfony-cmf/routing-bundle": "^2.0",
        "twbs/bootstrap": ">2.0,<4.0-dev",
        "twig/twig": "^1.0||^2.0||^3.0"
    },
    "scripts": {
    },
    "autoload": {
        "psr-4": {
            "Networking\\InitCmsBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Networking\\InitCmsBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "require-dev": {
        "dg/bypass-finals": "^1.1",
        "symfony/phpunit-bridge": "^5.0"
    }
}
