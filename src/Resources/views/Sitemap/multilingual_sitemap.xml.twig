<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for lang in languages %}
        <sitemap>
            <loc>{{ domain }}{{ path('xml_sitemap', {'locale': lang.locale}) }}</loc>
            <lastmod>{{ date()|date("c") }}</lastmod>
        </sitemap>
    {% endfor %}
</sitemapindex>