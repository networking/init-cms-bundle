<form method="post" novalidate
      action="{{ admin.generateUrl('editPageSettings', {'id': admin.id(object), 'uniqid': admin.uniqid, 'subclass': app.request.get('subclass')}) }}">
    {% if form.vars.errors|length > 0 %}
            {{ form_errors(form) }}
    {% endif %}
    {% set form_group = admin.formgroups.page_settings %}
    {% for field_name in form_group.fields %}
        {% if admin.formfielddescriptions[field_name] is defined %}
            {{ form_row(form[field_name]) }}
        {% endif %}
    {% endfor %}
    <input name="{{ form['_token'].vars.full_name }}" type="hidden" value="{{ form['_token'].vars.data }}" />
    <input type="submit" class="btn btn-primary" name="btn_update" id="pageSettingsSubmit"
            loading-text="{{ 'btn_loading'|trans({}, 'NetworkingInitCmsBundle') }}" value="{{ 'btn_update'|trans({}, 'SonataAdminBundle') }}"/>
</form>