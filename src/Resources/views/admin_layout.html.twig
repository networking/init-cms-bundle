    <!--begin::Header Mobile-->
    <div id="kt_header_mobile" class="header-mobile align-items-center header-mobile-fixed">
        <!--begin::Logo-->
        <a href="{{ url('sonata_admin_dashboard') }}">
            <img alt="Logo" class="max-h-25px"
