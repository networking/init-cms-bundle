    {% block page_scripts %}
    {% endblock %}
    <!--end::Page Vendors-->

    <!--begin::Page Scripts(used by this page)-->
    <script src="{{ asset('bundles/networkinginitcms/assets/js/pages/widgets.js') }}"></script>
    <!--end::Page Scripts-->
