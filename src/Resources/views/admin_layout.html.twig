        <!--begin::Main-->
    <div class="d-flex flex-column flex-root">
        {% block main %}
        <!--begin::Page-->
        <div class="d-flex flex-row flex-column-fluid page">

            <!--[html-partial:include:{"file":"partials/_aside.html"}]/-->
            {% block sidebar_left %}
                <!--begin::Aside-->
                <div class="aside aside-left aside-fixed d-flex flex-column flex-row-auto" id="kt_aside">

                    <!--begin::Brand-->
                    <div class="brand flex-column-auto" id="kt_brand">

                        <!--begin::Logo-->
                        <a href="{{ url('sonata_admin_dashboard') }}" class="brand-logo">
                            <img alt="Logo" class="max-h-25px"
                                 src="{{ admin_pool.titlelogo ? admin_pool.titlelogo : asset('bundles/networkinginitcms/img/initcms-396x100.png') }}"/>
                        </a>

                        <!--end::Logo-->

                        <!--begin::Toggle-->
                        <button class="brand-toggle btn btn-sm px-0" id="kt_aside_toggle">
							<span class="svg-icon svg-icon svg-icon-xl">

								<!--begin::Svg Icon | path:assets/media/svg/icons/Navigation/Angle-double-left.svg-->
								<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
                                     width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
									<g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
										<polygon points="0 0 24 0 24 24 0 24"/>
										<path d="M5.29288961,6.70710318 C4.90236532,6.31657888 4.90236532,5.68341391 5.29288961,5.29288961 C5.68341391,4.90236532 6.31657888,4.90236532 6.70710318,5.29288961 L12.7071032,11.2928896 C13.0856821,11.6714686 13.0989277,12.281055 12.7371505,12.675721 L7.23715054,18.675721 C6.86395813,19.08284 6.23139076,19.1103429 5.82427177,18.7371505 C5.41715278,18.3639581 5.38964985,17.7313908 5.76284226,17.3242718 L10.6158586,12.0300721 L5.29288961,6.70710318 Z"
                                              fill="#000000" fill-rule="nonzero"
                                              transform="translate(8.999997, 11.999999) scale(-1, 1) translate(-8.999997, -11.999999)"/>
										<path d="M10.7071009,15.7071068 C10.3165766,16.0976311 9.68341162,16.0976311 9.29288733,15.7071068 C8.90236304,15.3165825 8.90236304,14.6834175 9.29288733,14.2928932 L15.2928873,8.29289322 C15.6714663,7.91431428 16.2810527,7.90106866 16.6757187,8.26284586 L22.6757187,13.7628459 C23.0828377,14.1360383 23.1103407,14.7686056 22.7371482,15.1757246 C22.3639558,15.5828436 21.7313885,15.6103465 21.3242695,15.2371541 L16.0300699,10.3841378 L10.7071009,15.7071068 Z"
                                              fill="#000000" fill-rule="nonzero" opacity="0.3"
                                              transform="translate(15.999997, 11.999999) scale(-1, 1) rotate(-270.000000) translate(-15.999997, -11.999999)"/>
									</g>
								</svg>

                                <!--end::Svg Icon-->
							</span>
                        </button>

                        <!--end::Toolbar-->
                    </div>
                    <!--begin::Aside Menu-->
                    <div class="aside-menu-wrapper flex-column-fluid" id="kt_aside_menu_wrapper">
                        <!--begin::Menu Container-->
                        <div id="kt_aside_menu" class="aside-menu my-4" data-menu-vertical="1" data-menu-scroll="1"
                             data-menu-dropdown-timeout="500">
                            {% block side_bar_nav %}
                                {{ knp_menu_render('sonata_admin_sidebar', {template: '@NetworkingInitCms/Menu/admin_side_menu.html.twig', }) }}
                            {% endblock %}
                        </div>
                    </div>
                </div>




            {% endblock sidebar_left %}

            <!--begin::Wrapper-->
            <div class="d-flex flex-column flex-row-fluid wrapper" id="kt_wrapper">

                {% block init_cms_top_nav %}
                    {% include '@NetworkingInitCms/Navbar/admin_navbar.html.twig' %}
                {% endblock init_cms_top_nav %}

                <!--begin::Content-->
                <div class="content d-flex flex-column flex-column-fluid" id="kt_content">
                    {% block formstart %}{% endblock %}
                    {% block page_title %}
                        <div class="subheader py-2 py-lg-6 subheader-solid" id="kt_subheader">
                            <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">

                                <!--begin::Info-->
                                <div class="d-flex align-items-center flex-wrap mr-1">

                                    <!--begin::Page Heading-->
                                    <div class="d-flex align-items-baseline flex-wrap mr-5">

                                        <!--begin::Page Title-->
                                        <h5 class="text-dark font-weight-bold my-1 mr-5">
                                            {% if _title is not empty or action is defined %}
                                                {% if _title is not empty %}
                                                    {{ _title|raw }}
                                                {% else %}
                                                    {% if action is defined %}
                                                        {% for menu in breadcrumbs_builder.breadcrumbs(admin, action) %}
                                                            {% if loop.last %}
                                                                {%- set translation_domain = menu.extra('translation_domain', 'messages') -%}
                                                                {%- set label = menu.label -%}
                                                                {%- if translation_domain is not same as(false) -%}
                                                                    {%- set label = label|trans(menu.extra('translation_params', {}), translation_domain) -%}
                                                                {%- endif -%}
                                                                {{ label|raw }}
                                                            {% endif %}
                                                        {% endfor %}
                                                    {% endif %}
                                                    {% if action == 'edit' %}
                                                        {% set settings = {} %}
                                                        {% if object.locale is defined %}
                                                            {% set settings = {'locale': object.locale} %}
                                                        {% endif %}

                                                        <a class="link-underlined font-small font-light"
                                                           href="{{ admin.generateUrl('list', settings) }}">{{ 'link.go_to_list'|trans({'%admin_label%':admin.label}, 'NetworkingInitCmsBundle') }}</a>
                                                    {% endif %}
                                                {% endif %}

                                            {% endif %}
                                        </h5>

                                        <!--end::Page Title-->
                                    </div>

                                    <!--end::Page Heading-->
                                </div>

                                <!--end::Info-->
                                {% block center_sub_title %}{% endblock %}
                                <!--begin::Toolbar-->
                                <div class="d-flex align-items-center">

                                    <!--begin::Actions-->
                                    <!--end::Actions-->

                                        {% block top_actions %}{{ _actions|raw }}{% endblock %}
                                    <div class="ml-6">
                                        {% if app.user  and is_granted('ROLE_SONATA_ADMIN') %}
                                            {% if _help_text is empty %}
                                                {% if admin is not defined %}
                                                    {% set adminCode = 'dashboard' %}
                                                    {% set actionHelp = '' %}
                                                {% else %}
                                                    {% set adminCode = admin.code %}
                                                    {% set actionHelp = action %}
                                                {% endif %}
                                                <a href="{{ url('networking_init_cms_help', {'adminCode':adminCode,'action':actionHelp}) }}"
                                                   target="_blank">
                                                    <i class="icon-2x flaticon-info"></i>
                                                </a>
                                            {% endif %}
                                        {% endif %}
                                    </div>
                                    <!--end::Dropdown-->
                                </div>

                                <!--end::Toolbar-->

                            </div>

                        </div>
                    {% endblock %}

                    <!--[html-partial:include:{"file":"partials/_content.html"}]/-->
                    {% block sonata_admin_content %}
                        <div class="d-flex flex-column-fluid">
                            <div class="container-fluid">

                                {% if _preview is not empty %}
                                    {{ _preview|raw }}
                                {% endif %}

                                {% if _content is not empty %}
                                    {{ _content|raw }}
                                {% endif %}

                                {% if _help_text is not empty %}
                                    {{ _help_text|raw }}
                                {% endif %}

                                {% if _show is not empty %}
                                    {{ _show|raw }}
                                {% endif %}

                                {% if _form is not empty %}
                                    {{ _form|raw }}
                                {% endif %}

                                {% if _list_table is not empty or _list_filters is not empty or (_actions is not empty and _show is not empty) %}
                                    <div class="card card-custom">
                                        <div class="card-body">
                                            {{ _list_filters|raw }}
                                            {{ _list_table|raw }}
                                        </div>
                                    </div>
                                {% endif %}
                                {% if _side_menu is not empty %}
                                    <div class="sidebar col-md-2">
                                        <div class="sonata-ba-side-menu">{{ _side_menu|raw }}</div>
                                    </div>
                                {% endif %}
                            </div>
                        </div>
                    {% endblock %}

                    <!--end::Content-->
                    {% block formend %}{% endblock %}
                </div>
                <!--[html-partial:include:{"file":"partials/_footer.html"}]/-->
                {% block footer %}
                    {{ sonata_block_render({ 'type': 'networking_init_cms.block.service.version' }, {}) }}
                {% endblock footer %}
                <!--end::Wrapper-->
            </div>
        </div>
        <!--end::Page-->
        {% endblock main %}
    </div>
