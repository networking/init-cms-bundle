{% if admin.isGranted('DELETE', object) and admin.hasRoute('delete') and (object.isDeletable() is not defined or  object.isDeletable == 1 ) %}
    <a href="{{ admin.generateObjectUrl('delete', object) }}"
       class="{% if isSubNav is defined  %}nav-link{% else %}btn btn-sm btn-clean btn-icon{% endif %} show-tooltip dialog-link"
       id="{{ admin.getClassnameLabel() }}_{{ object.id }}"
       data-original-title="{{ 'action_delete'|trans({}, 'SonataAdminBundle') }}"
       title="{{ 'action_delete'|trans({}, 'SonataAdminBundle') }}"><i class="nav-icon la la-trash"></i>
        {% if isSubNav is defined  %}
        <span class="nav-text">{{ 'action_delete'|trans({}, 'SonataAdminBundle') }}</span>
        {% endif %}
    </a>
{% endif %}
