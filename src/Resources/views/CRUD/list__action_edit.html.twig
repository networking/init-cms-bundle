{% if admin.isGranted('EDIT', object) and admin.hasRoute('edit') %}
<a  href="{{ admin.generateObjectUrl('edit', object) }}"
    class="{% if isSubNav is defined %}menu-link px-3{% else %} btn btn-light btn-sm button-in-row {% endif %}"
    {% if isSubNav is not defined %}data-bs-toggle="tooltip" data-bs-placement="bottom"{% endif %}
    title="{{ 'action_edit'|trans({}, 'SonataAdminBundle') }}" data-page_id="{{ admin.id(object) }}">
  <i class="ki-outline ki-pencil fs-3 menu-icon"></i>{% if isSubNav is defined  %}{{ 'action_edit'|trans({}, 'SonataAdminBundle') }}
  {% endif %}
</a>
{% endif %}