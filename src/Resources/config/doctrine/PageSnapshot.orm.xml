<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Networking\InitCmsBundle\Entity\PageSnapshot" table="page_snapshot">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="resourceName" type="string" column="resource_name"/>
    <field name="resourceId" type="integer" column="resource_id"/>
    <field name="versionedData" type="array" column="versioned_data"/>
    <field name="version" type="integer" column="version"/>
    <field name="snapshotDate" type="datetime" column="snapshot_date"/>
    <field name="path" type="string" column="path" length="255" nullable="true"/>
    <many-to-one field="contentRoute" target-entity="Networking\InitCmsBundle\Entity\ContentRoute" fetch="LAZY">
      <cascade>
        <cascade-remove/>
        <cascade-persist/>
      </cascade>
      <join-columns>
        <join-column name="content_route_id" referenced-column-name="id" on-delete="CASCADE"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
