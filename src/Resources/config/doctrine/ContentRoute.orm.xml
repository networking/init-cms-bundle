<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Networking\InitCmsBundle\Entity\ContentRoute" table="content_route">
    <unique-constraints>
      <unique-constraint name="content_route_idx" columns="path,locale,class_type"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255"/>
    <field name="classType" type="string" column="class_type" length="255"/>
    <field name="objectId" type="integer" column="object_id" nullable="true"/>
    <field name="path" type="string" column="path" length="255" nullable="true"/>
    <field name="controller" type="string" column="controller" length="255" nullable="true"/>
    <field name="template" type="string" column="template" length="255" nullable="true"/>
    <field name="templateName" type="string" column="template_name" length="255" nullable="true"/>
    <field name="locale" type="string" column="locale" length="6"/>
  </entity>
</doctrine-mapping>
