<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Networking\InitCmsBundle\Entity\LayoutBlock" table="layout_block">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="true"/>
    <field name="zone" type="string" column="zone" length="255"/>
    <field name="classType" type="string" column="class_type" length="255"/>
    <field name="objectId" type="integer" column="object_id" nullable="true"/>
    <field name="isActive" type="boolean" column="is_active"/>
    <field name="sortOrder" type="integer" column="sort_order" nullable="true"/>
    <field name="createdAt" type="datetime" column="created_at"/>
    <field name="updatedAt" type="datetime" column="updated_at"/>
    <lifecycle-callbacks>
      <lifecycle-callback type="prePersist" method="prePersist"/>
      <lifecycle-callback type="preUpdate" method="preUpdate"/>
    </lifecycle-callbacks>
  </entity>
</doctrine-mapping>
